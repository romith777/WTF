{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/products",
      "dest": "server.js"
    },
    {
      "src": "/signup",
      "dest": "server.js"
    },
    {
      "src": "/login",
      "dest": "server.js"
    },
    {
      "src": "/cart(.*)",
      "dest": "server.js"
    },
    {
      "src": "/favorites(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
